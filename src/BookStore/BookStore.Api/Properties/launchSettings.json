{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5006"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "books",
        "environmentVariables": {
            "ASPNETCORE_ENVIRONMENT": "Development",
            "ConnectionStrings__BookstoreDbContext": "Server=localhost;Database=Bookstore;User Id=bookstore;Password=bookstore;TrustServerCertificate=true"
        },
      "dotnetRunMessages": true,
       "applicationUrl": "https://localhost:7253;http://localhost:5006"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
        "environmentVariables": {
            "ASPNETCORE_HTTPS_PORTS": "8081",
            "ASPNETCORE_HTTP_PORTS": "8080",
            "ConnectionStrings__BookstoreDbContext": "Server=localhost;Database=Bookstore;User Id=bookstore;Password=bookstore;TrustServerCertificate=true"
        },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:21950",
      "sslPort": 44340
    }
  }
}