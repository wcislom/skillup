{
    "Logging": {
        "Console": {
            "FormatterName": "json",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "HH:mm:ss ",
                "UseUtcTimestamp": true,
                "JsonWriterOptions": {
                    "Indented": true
                }
            },
            "LogLevel": {
                "Default": "Information",
                "Microsoft.AspNetCore": "Information",
                "Microsoft.EntityFrameworkCore": "Debug",
                "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"

            }
        },
        "Seq": {
            "ServerUrl": "http://localhost:5341",
            "LogLevel": { 
                "System": "Information",
                "Microsoft": "Warning",
                "BookStore": "Trace"
            }
        }
    },
  "AllowedHosts": "*"
}
